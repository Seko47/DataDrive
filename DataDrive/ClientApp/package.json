{
  "name": "datadrive",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng run DataDrive:server:dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-pl-translation": "ngx-translate-extract --input ./src --output ./src/assets/i18n/pl.json --clean --sort --format namespaced-json",
    "extract-en-translation": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json --clean --sort --format namespaced-json",
    "extract-translations": "npm run extract-pl-translation && npm run extract-en-translation"
  },
  "private": true,
